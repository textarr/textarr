{
  "name": "textarr",
  "version": "1.0.0",
  "description": "SMS bot for requesting movies and TV shows via Sonarr/Radarr",
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.50",
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/openai": "^2.0.75",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/formbody": "^8.0.1",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/secure-session": "^8.2.0",
    "@fastify/static": "^8.0.3",
    "@slack/bolt": "^4.6.0",
    "ai": "^5.0.104",
    "bcrypt": "^6.0.0",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.1.0",
    "grammy": "^1.38.4",
    "pino": "^10.1.0",
    "twilio": "^5.3.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.16.0",
    "globals": "^16.5.0",
    "msw": "^2.6.8",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.48.0",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "sonarr",
    "radarr",
    "sms",
    "twilio",
    "media",
    "bot"
  ],
  "license": "MIT"
}
